<div class="background-Sendapplication-black">
    <h1 class="title-MYapplication">Оставить заявку</h1>
    <div class="container appliction-container-main">
        <form name="FormAddApplication" class="row g-3" action="#" method="#">
            <div class="col-md-6">
                <label for="inputEmail4" class="form-label home-footer">Цель обследования</label>
                <input type="text" class="form-control" name="target" required>
            </div>
            <div class="col-md-6">
                <label for="inputPassword4" class="form-label home-footer">Причина обследования</label>
                <input type="text" class="form-control" name="reason" required>
            </div>
            <div class="col-12">
                <label for="inputAddress" class="form-label home-footer">Название объекта</label>
                <input type="text" class="form-control" name="title"
                    placeholder="ОАО «Мозырский нефтеперерабатывающий завод»" required>
            </div>
            <div class="col-12">
                <label for="inputAddress2" class="form-label home-footer">Адрес объекта</label>
                <input type="text" class="form-control" name="adres" placeholder="ZHukovskogo Ul., bld. 10/1, appt. 64"
                    required>
            </div>
            <div class="col-md-6">
                <label for="inputCity" class="form-label home-footer">Почта</label>
                <input type="email" class="form-control" name="email">
            </div>
            <div class="col-12">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" id="gridCheck" required>
                    <label class="form-check-label home-footer" for="gridCheck ">
                        Check me out
                    </label>
                </div>
            </div>
            <div class="col-12" id="sendApplicationHome">
                <input type="submit" class="btn btn-primary" id="submitaddApplication">
            </div>
        </form>

    </div>
</div>
